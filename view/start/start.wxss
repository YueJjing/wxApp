/* pages/start/start.wxss */
.start{
  position: fixed;
  width: 100%;
  height: 100%;
}
.start text{
  width: 120rpx;
  height: 80rpx;
  line-height: 80rpx;
  display: block;
  position: absolute;
  z-index: 1;
  top:50%;
  right: 40%;
  /* margin-right: -50%; */
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  border-radius: 20rpx;
  cursor: pointer;
}
.swiper{
  position: absolute;
  width: 100%;
  height: 100%
}
.swiper image{
  width: 100%;
  height: 100%
}